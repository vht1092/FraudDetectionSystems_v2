			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="s_todate" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString language="SQL">
			<![CDATA[select d.case_no,
       substr(d.cre_tms,7,2)||'/'||substr(d.cre_tms,5,2)||'/'||substr(d.cre_tms,1,4)||' '||substr(d.cre_tms,9,2)||':'||substr(d.cre_tms,11,2)||':'||case when substr(d.cre_tms,13,2)>'59' then '59' else substr(d.cre_tms,13,2) end as cre_tms,
       (select px_irpanmap_panmask
          from ir_pan_map@im.world p
         where p.px_irpanmap_pan = d.enc_crd_no) panmask,
       (select F9_DW005_LOC_ACCT
          from ccps.dw005
         where PX_DW005_PAN = d.enc_crd_no
        union
        select F9_DW006_LOC_ACCT
          from ccps.dw006
         where PX_DW006_own_PAN = d.enc_crd_no) loc,cif_no as CIF,
       (select listagg(r.rule_id, ',') within group(order by r.case_no) a
          from ccps.fds_case_hit_rules r
         where r.case_no = d.case_no
         group by r.case_no) ruleid,
       des2.description case_status
  from ccps.fds_case_detail d, ccps.fds_description des2
 where d.cre_tms between  to_number($P{s_fromdate})  and  to_number($P{s_todate})
   and d.case_status = des2.id
   and des2.type = 'CASE STATUS'
 order by d.cre_tms desc]]>
		</queryString>
		<field name="CASE_NO" class="java.lang.String"/>
		<field name="CRE_TMS" class="java.lang.String"/>
		<field name="PANMASK" class="java.lang.String"/>
		<field name="LOC" class="java.math.BigDecimal"/>
		<field name="RULEID" class="java.lang.String"/>
		<field name="CASE_STATUS" class="java.lang.String"/>
		<field name="CIF" class="java.lang.String"/>
	</subDataset>
