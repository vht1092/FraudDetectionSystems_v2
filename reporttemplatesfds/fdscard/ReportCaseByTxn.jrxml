		<parameter name="S_CARDNO" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString language="SQL">
			<![CDATA[select
       m.cif,
       m.pan_mask,
       m.loc,
       m.custname,
       m.crd_brn,
       m.crd_pgm,
       m.txn_time,
       m.ori_amt,
       m.crncy_cde,
       m.vnd_amt,
       m.merc_name,
       m.pos_mode,
       case when m.ref_cde like '%20%' then 'Y' else 'N' end as temp_lock,
       m.mcc,
       m.case_no,
       m.ruleid,
       (select case when ID = 'NCO' then 'A11'
                    when ID = 'TR2' then 'A12'
                    when ID = 'CCT' then 'A13'
                    when ID = 'BCC' then 'A14'
                    when ID = 'CCP' then 'A15'
                    when ID = 'CCS' then 'A16'
                    when ID = 'CCN' then 'A17'
                    when ID = 'NCR' then 'A18'
                    when ID = 'HCS' then 'A19'
                    when ID = 'HCF' then 'A20'
                    when ID = 'CHL' then 'A21'
                    when ID = 'CNC' then 'A22'
                    when ID = 'NCT' then 'A23'
                    when ID = 'SAM' then 'A24'
                    when ID = 'ACO' then 'A25'
                    when ID = 'CON' then 'A26'
                    when ID = 'SEM' then 'A27'
                    when ID = 'TRA' then 'A28'
                    when ID = 'BLC' then 'A29'
                END
       ||'-'||DESCRIPTION from ccps.fds_description where TYPE = 'ACTION' and ID = m.case_action_id) case_action,
       m.closed_reason,
       (select des2.description
          from ccps.fds_description des2
         where des2.type = 'CASE STATUS'
           and des2.id = m.case_status) case_status,
       m.usr_id
  from (select d.cif_no as cif,
               px_irpanmap_panmask as pan_mask,
               (select f9_dw005_loc_acct
                  from ccps.dw005
                 where px_dw005_pan = d.enc_crd_no
                union
                select f9_dw006_loc_acct
                  from ccps.dw006
                 where px_dw006_own_pan = d.enc_crd_no) loc,
               (select trim(fx_dw005_emb_lst_nm) || ' ' ||
                       trim(fx_dw005_emb_mid_nm) || ' ' ||
                       trim(fx_dw005_emb_name)
                  from ccps.dw005
                 where px_dw005_pan = d.enc_crd_no
                union
                select trim(fx_dw006_emb_lst_nm) || ' ' ||
                       trim(fx_dw006_emb_mid_nm) || ' ' ||
                       trim(fx_dw006_emb_name)
                  from ccps.dw006
                 where px_dw006_own_pan = d.enc_crd_no) custname,
               d.crd_brn,
               to_char(to_date(substr(d.txn_cre_tms, 0, 14),
                               'yyyyMMddHH24MISS'),
                       'dd/mm/yyyy HH24:MI:SS') txn_time,
               d.case_no,
               (select listagg(r.rule_id, ', ') within group(order by r.case_no) a
                  from ccps.fds_case_hit_rules r
                 where r.case_no = d.case_no
                 group by r.case_no) ruleid,
               (select t.closed_reason
                  from ccps.fds_case_status t
                 where t.cre_tms =
                       (select max(t2.cre_tms)
                          from ccps.fds_case_status t2
                         where t2.case_no = t.case_no)
                   and t.case_no = d.case_no and rownum = 1) case_action_id,
               (select REPLACE(REPLACE(t.case_comment, CHR(10),''),CHR(13),'')
                  from ccps.fds_case_status t
                 where t.cre_tms =
                       (select max(t2.cre_tms)
                          from ccps.fds_case_status t2
                         where t2.case_no = t.case_no)
                   and t.case_no = d.case_no and rownum = 1) closed_reason,
               d.case_status,
               (select t.user_id
                  from ccps.fds_case_status t
                 where t.cre_tms =
                       (select max(t2.cre_tms)
                          from ccps.fds_case_status t2
                         where t2.case_no = t.case_no)
                   and t.case_no = d.case_no and rownum = 1) usr_id,
               fdstxndetail.f9_oa008_ori_amt as ori_amt,
               fdstxndetail.fx_oa008_pos_mode as pos_mode,
               fdstxndetail.fx_oa008_ref_cde as ref_cde,
               fdstxndetail.f9_oa008_crncy_cde as crncy_cde,
               fdstxndetail.f9_oa008_amt_req + fdstxndetail.f9_oa008_load_fee as vnd_amt,
               fdstxndetail.fx_oa008_merc_name as merc_name,
               fdstxndetail.f9_oa008_mcc as mcc,
               fdstxndetail.fx_oa008_crd_pgm as crd_pgm
          from ccps.fds_case_detail d
          join ccps.fds_txn_detail fdstxndetail
            on d.txn_cre_tms = fdstxndetail.f9_oa008_cre_tms
           and d.enc_crd_no = fdstxndetail.fx_oa008_used_pan
          left join ir_pan_map@im.world p
            on p.px_irpanmap_pan = d.enc_crd_no
         where 1 = 1
           and d.txn_cre_tms between  to_number($P{S_FROMDATE})  and to_number($P{S_TODATE})
           and ($P{S_CRDTYPE} is null or substr(fdstxndetail.fx_oa008_crd_pgm,1,2) = $P{S_CRDTYPE})
           and ($P{S_CARDNO} IS NULL OR px_irpanmap_panmask = $P{S_CARDNO})
         order by d.txn_cre_tms desc
         ) m]]>
		</queryString>
